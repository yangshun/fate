{
  "name": "@nkzw/fate",
  "version": "0.0.0",
  "description": "Fate is a modern data client for tRPC and React.",
  "homepage": "https://github.com/nkzw-tech/fate",
  "repository": {
    "type": "git",
    "url": "https://github.com/nkzw-tech/fate"
  },
  "license": "MIT",
  "author": {
    "name": "Christoph Nakazawa",
    "email": "christoph.pojer@gmail.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.mts",
      "development": "./src/index.ts",
      "default": "./lib/index.mjs"
    },
    "./cli": {
      "types": "./lib/cli.d.mts",
      "development": "./src/cli.ts",
      "default": "./lib/cli.mjs"
    },
    "./server": {
      "types": "./lib/server.d.mts",
      "development": "./src/server.ts",
      "default": "./lib/server.mjs"
    }
  },
  "main": "./lib/index.mjs",
  "types": "./lib/index.d.mts",
  "bin": {
    "fate": "./lib/cli.mjs"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsdown -d lib --target=node24 src/index.ts src/server.ts src/cli.ts"
  },
  "dependencies": {
    "superjson": "^2.2.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1"
  },
  "peerDependencies": {
    "@trpc/client": "^11.6.0"
  }
}
